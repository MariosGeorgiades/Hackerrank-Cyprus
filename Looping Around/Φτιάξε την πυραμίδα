#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int a;
    cin>>a;
        
    for(int i = 1; i<= a; ++i)
    {
        for(int j = 1; j <= i; ++j)
        {
            cout<<j;
        }
        cout<<"\n";   
    
    }
    
    
    return 0;
}
