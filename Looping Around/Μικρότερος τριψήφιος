#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
int a, sum=-1;       
cin>>a;
    for(int i=100;i<=999; i++){
        int c, b, e;
        c=i/100;
        b=i/10%10;
        e=i%10;
        if(c*b*e == a){
            sum=i;
        break; }
    }
        
   cout<<sum;
}
